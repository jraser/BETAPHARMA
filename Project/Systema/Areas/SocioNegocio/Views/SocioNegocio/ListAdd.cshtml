
<div class="portlet-body">
    <div class="table-responsive">
        <div class="col-md-12 portlet-body  ">
            <div class="modal-header">
                <h3 class="modal-title">Búsqueda de Proveedor</h3>
            </div>
            <div class="modal-body">
                <form role="form">
                    <div class="form-group">
                        <label class="control-label">Razon Social</label>
                        <input type="text" class="form-control input-medium" placeholder=""
                               ng-model="parameters.Nombres">
                    </div>
                    <div class="form-group">
                        <label class="control-label">RUC</label>
                        <input type="text" class="form-control input-medium" placeholder=""
                               ng-model="parameters.Nro_Documento">
                        <button type="submit" class="btn green" ng-click="List()">Buscar</button>
                    </div>

                </form>
            </div>
            <table class="table <!--table-striped--> table-bordered table-advance table-hover">
                <thead>
                    <tr>
                        <th>
                            ID
                        </th>
                        <th>
                            Razon Social
                        </th>                       
                        <th>
                            RUC
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="item in SocioNegocioList">
                        <td>{{item.ID_Socio}}</td>
                        <td>{{item.Nombre}}</td>
                        <td>{{item.Num_Documento}}</td>
                        <td align="center">
                            <input type="radio"
                                   ng-click="Select(item)"
                                   ng-checked="item.checked">
                        </td>
                    </tr>
                </tbody>
            </table>








            <div class="modal-footer">
                <button class="btn btn-primary" ng-click="ok()">Aceptar</button>
                <button class="btn btn-warning" ng-click="cancel()">Cancelar</button>
            </div>
        </div>





    </div>
</div>

