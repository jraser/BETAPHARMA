

<h1>GESTIÓN DE SOCIOS</h1>
<div class="page-bar blue-dark">


    <a ng-click="Create()" class="btn default">
        Nuevo
    </a>
    <a ng-click="Edit()" class="btn default">
        Modificar
    </a>
    <a ng-click="Deletes()" class="btn default">
        Desactivar
    </a>

</div>


<div class="row">
    <div class="col-md-12">
        <div class="portlet box blue-dark ">

            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-gift"></i>Búsqueda de Socio
                </div>
                <div class="tools">
                    <a href="" class="collapse">
                    </a>

                    <a href="" class="reload">
                    </a>

                </div>
            </div>



            <div class="portlet-body" id="blockui_List_portlet_body">
                <div class="table-responsive">

                   
                    <div class="page-bar blue-dark ">
                        <form role="form" class="form-inline" name="FormSocioNegocioList">
                            <div class="form-group">
                                <label class="control-label">Nombres</label>
                                <input type="text" class="form-control input-medium" placeholder=""
                                       ng-model="parameters.Nombres">

                                <label class="control-label">Numero Documento</label>
                                <input type="text" class="form-control input-medium" placeholder=""
                                       ng-model="parameters.Nro_Documento">

                                <label class="control-label">Tipo de Socio</label>
                                <select ng-model="parameters.TipoSocio" class="form-control">
                                    <option value='%'>Todos</option>
                                    <option value='C'>Cliente</option>
                                    <option value='P'>Proveedor</option>
                                </select>
                            </div>
                            <div class="form-group">
                                <button type="submit" class="btn blue-dark" ng-click="List()">Buscar</button>
                            </div>
                        </form>
                    </div>




                    <div>
                        <div class="row">

                        </div>

                    </div>

                    <div>
                        <div class="row">

                        </div>

                    </div>
                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>


                                <th>
                                    ID
                                </th>
                                <th>
                                    TIPO SOCIO
                                </th>
                                <th>
                                    REGIMEN
                                </th>
                                <th>
                                    TIPO DOCUMENTO
                                </th>
                                <th>
                                    N°DE DOCUMENTO
                                </th>
                                <th>
                                    NOMBRE SOCIO
                                </th>
                                <th>
                                    DIRECCION
                                </th>
                                <th>
                                    TELEFONO
                                </th>
                                
                          
                                <th></th>

                            </tr>
                        </thead>


                        <tr ng-repeat="item in SocioNegocioList">

                            <td>{{item.ID_Socio}}</td>

                            <td>{{item.Tipo_Socio}}</td>

                            <td>{{item.Tipo_Regimen}}</td>

                            <td>{{item.Tipo_Documento}}</td>

                            <td>{{item.Num_Documento}}</td>

                            <td>{{item.Nombre}}</td>

                            <td>{{item.DIRECCION}}</td>
                            <td>{{item.Telefono}}</td>
                          
                            <td align="center">
                                <input type="checkbox"
                                       ng-click="select(item.ID_Socio)">
                            </td>
                        </tr>
                    </table>

                </div>
            </div>

        </div>



    </div>
</div>











